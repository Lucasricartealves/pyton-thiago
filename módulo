#função para somar dois valores 
def somar(valueA, valueB):
  print(valueA + valueB)
  
#função para subtrair dois valores
def subtrair(valueA, valueB):
  print(valueA - valueB)
  
#função para multiplicar dois valores
def multiplicar(valueA, valueB):
  print(valueA * valueB)
  
#função para dividir dois valores
def dividir(valueA, valueB):
  print(valueA / valueB)

